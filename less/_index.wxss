}
.account-nav {
  margin: 0 auto;
}
@media (max-width: 768px) {
  .account-nav {
    display: flex;
    flex-wrap: wrap;
  }
  .account-nav [class*="col-"] {
    width: 50%;
  }
}
.message-images {
  padding-top: 40px;
  margin: 0 auto;
}
