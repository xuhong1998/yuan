  .message-right {
    display: none;
  }
}
.message-right p:nth-child(1) {
  margin-top: 10px;
  font-size: 20px;
  font-weight: 600;
  margin-bottom: 0px;
  color: #ffffff;
}
.message-right p:nth-child(2) {
  font-size: 9px;
