  .about-images [class*="col-sm-"] + [class*="col-sm-"] {
    padding-left: 0;
  }
}
.about-images .wow {
  padding-left: 0px;
}
.text-account h3 {
  text-align: center;
  font-size: 20px;
  font-weight: bold;
  margin: 25px 0px 25px 0px;
}
.text-account p {
  text-align: center;
  margin: 0px;
